# Required
BASE_URL=https://your-site.com

# Registration flow
REGISTER_PATH=/en/user/register
DASHBOARD_URL_PATTERN=/en/generate
MAX_RETRIES=3
RETRY_DELAY_SECONDS=3
TIMEOUT_MS=30000
HEADLESS=true
LOCALE=en_US

# Artifacts
VIDEO_DIR=test_recordings
ARTIFACT_DIR=artifacts

# Form selectors
EMAIL_SELECTOR=input[name="email"]
PASSWORD_SELECTOR=input[name="password"]
NAME_SELECTOR=input[name="name"]
SUBMIT_SELECTOR=button[type="submit"]
ERROR_SELECTOR=.error-message, .alert-danger, [role='alert']

# Optional Qwen reasoning (OpenAI-compatible endpoint)
QWEN_ENABLED=false
QWEN_MODEL_SERVER=http://localhost:11434/v1
QWEN_MODEL=Qwen/Qwen3-14B
QWEN_API_KEY=EMPTY
QWEN_MAX_TOKENS=512
QWEN_ENABLE_THINKING=true
